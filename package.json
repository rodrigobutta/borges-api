  "homepage": "https://github.com/rodrigobutta/borges#readme",
  "scripts": {
    "commit:patch": "npm version patch --no-git-tag-version --force && PACKAGE_VERSION=$(node -p \"require('./package.json').version\") && git add . && git commit -m \"TAG PATCH\" && git tag $PACKAGE_VERSION HEAD && git push --tags && git push origin",
    "commit:minor": "npm version minor --no-git-tag-version --force && PACKAGE_VERSION=$(node -p \"require('./package.json').version\") && git add . && git commit -m \"TAG MINOR\" && git tag $PACKAGE_VERSION HEAD && git push --tags && git push origin",
    "commit:major": "npm version major --no-git-tag-version --force && PACKAGE_VERSION=$(node -p \"require('./package.json').version\") && git add . && git commit -m \"TAG MAJOR\" && git tag $PACKAGE_VERSION HEAD && git push --tags && git push origin",
    "build": "rimraf ./dist && tsc",
    "migrate": "npx sequelize db:migrate",
    "start": "node dist/server.js --port=8080",
    "dev": "nodemon --config nodemon.json ./src/server.ts --port=3090",
    "test": "mocha"
  },
  "nodemonConfig": {
    "execMap": {
      "ts": "node --require ts-node/register/transpile-only"
    }
  },
