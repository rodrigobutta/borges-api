{
  "name": "borges-api",
  "version": "1.1.10",
  "description": "Borges API",
  "author": "Borges <rbutta@borges.com>",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rodrigobutta/borges.git"
